@model IEnumerable<PredictiveOrdering.Models.Cust_Authenication>

@{
    ViewBag.Title = "Verizon New User";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Sign UP", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cust_Password)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cust_Password)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Cust_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Cust_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Cust_Id })
        </td>
    </tr>
}

</table>
